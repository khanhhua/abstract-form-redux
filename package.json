{
  "name": "abstract-form-redux",
  "version": "0.0.2",
  "description": "Abstract form extension library Redux",
  "main": "lib/index.js",
  "scripts": {
    "test:unit": "mocha --require ts-node/register --ui bdd src/**/*.spec.ts",
    "test:jest": "jest",
    "test": "npm run test:unit && npm run test:jest",
    "build": "rm -Rf lib/* && tsc -p ."
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/__tests__/test-preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/khanhhua/abstract-form.git"
  },
  "keywords": [
    "form",
    "configurable"
  ],
  "author": "Khanh Hua",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/khanhhua/abstract-form/issues"
  },
  "homepage": "https://github.com/khanhhua/abstract-form#readme",
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/chai-spies": "^1.0.0",
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.2",
    "@types/mocha": "^5.2.5",
    "@types/react-redux": "^6.0.9",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "jest": "^23.6.0",
    "mocha": "^5.2.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.5.2",
    "ts-mocha": "^2.0.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.0.3",
    "@types/enzyme-adapter-react-16": "^1.0.3"
  },
  "dependencies": {
    "abstract-form": "git+https://github.com/khanhhua/abstract-form.git"
  }
}
